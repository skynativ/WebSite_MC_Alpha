					"Gérer les paiements": {
						"icon": "credit-card",
						"permission": "SHOP__ADMIN_MANAGE_PAYMENT",
						"route": "/admin/shop/payment"
					}
				}
			}
		}
	},
	"navbar_routes": {
		"Boutique": "/shop"
	},
